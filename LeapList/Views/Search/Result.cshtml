@model List<LeapList.Models.CLItem>

@{
    ViewBag.Title = "Result";
}

<table>
    <tr>
        <th>Title</th>
        <th>Price</th>
    </tr>
    @for(var i = 0; i < Model.Count; i++)
    {
        <tr>
            <td>@Model[i].Title</td>
            <td>@Model[i].Price</td>
        </tr>
    }
</table>